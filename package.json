{
  "name": "reactnativeandroid",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "yarn bundle && yarn android",
    "serve": "yarn react-native start",
    "android": "react-native run-android",
    "bundle": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res/",
    "make:as": "mkdir android\\app\\src\\main\\assets"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-native": "^0.63.4",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-native": "^0.63.37",
    "metro": "^0.64.0",
    "metro-core": "^0.64.0",
    "react-native-typescript-transformer": "^1.2.13"
  }
}
