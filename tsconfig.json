{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["ES5", "ES6"],
    // "allowJs": true,
    "jsx": "react",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["./src/components/*"],
      "@screen/*": ["./src/screen/*"],
      "@types/*": ["./src/types/*"],
      "@dumy/*": ["./src/dumy/*"]
    }
  }
}
